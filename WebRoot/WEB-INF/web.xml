<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ShowBook</servlet-name>
    <servlet-class>com.hsp.ShowBook</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuyBookCl</servlet-name>
    <servlet-class>com.hsp.BuyBookCl</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowMyCart</servlet-name>
    <servlet-class>com.hsp.ShowMyCart</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>ShowBook</servlet-name>
    <url-pattern>/ShowBook</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuyBookCl</servlet-name>
    <url-pattern>/BuyBookCl</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowMyCart</servlet-name>
    <url-pattern>/ShowMyCart</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
